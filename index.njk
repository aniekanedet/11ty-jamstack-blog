  data: collections.posts
  size: 2
  alias: posts
  reverse: true
layout: layouts/base.njk
---
<div class="container ">
                           <div class="header-content">
                               <nav class="header-social-icons">
                              
                                            <li class="icon-link">
                                                <a class="" href="https://twitter.com/aniekanedett" target="_blank">
                                                            <i class="fab fa-twitter"></i>                            
                                                </a>                            
                                            </li>
                                            <li class="icon-link">
                                                <a class="" href="https://www.facebook.com/aniekan.edet.3/" target="_blank">
                                                            <i class="fab fa-facebook-f"></i>                            
                                                </a>                            
                                            </li>
                                            <li class="icon-link">
                                                <a class="" href="https://linkedin.com/in/aniekan-edet" target="_blank">
                                                            <i class="fab fa-linkedin-in"></i> 
                                                    </a>                             
                                            </li>
                                            <li class="icon-link">
                                                <a class="" href="https://github.com/aniekanedet" target="_blank">
                                                        <i class="fab fa-github"></i>                            
                                                </a>
                                            </li>
                                            
                               </nav>
                               
                                            <div class="typing-text">
                                                    <p id="type"></p>
                                            </div>
                          </div>
                          
                          <div class="header-icon">
                                <a class="animated pulse infinite delay-5s piont" href="#section1">
                                    <i class=" animated pulse infinite delay-5s  fa fa-arrow-down" aria-hidden="true"></i>
                                </a>
                        </div>
                       </div>
                   </section>
           </section >
            <div class="divider">
            </div>

               <section class="divider">
                   <div id="section1" class="container-fluid about-content">
                           <div class="container">
                                           <div class="row">
                                                  <div class="col-md-10 text-center">
                                                      <div class="about-info" >
                                                             <p>
                                                                Hi again <img src="assets/image/smile.png" alt="smile">, Thanks for coming around. <br> <br>

                                                                I’m a Frontend developer with 3 years of
                                                                experience, passionate about building amazing looking websites for small
                                                                businesses and personal brands to help grow their business online and
                                                                expose them to a wider range of customers.
                                                            
                                                            </p>
                                                               <div class="text-center mb-5">
                                    <a href="/about" ><button class="btn sendbtn text-center">Read More</button></a>
                                                        </div>

                                                      </div>
                                                  </div>
                                                  
                                               </div>
                                   </div>
                   </div>
               </section>
           
               
                <section class="sectional-content">
                   <div id="section6">
                       <div class="container ">
                            <h3 class="mt-4 mb-5 ">Notes </h3>
                <div class="row">
                    <div class="col-lg-8 col-md-10 mx-auto">
                    {# <pre>{{ collections.posts | dump }}</pre> #}

                    {%for post in posts%}
                    <div class="post-preview">
                        <a href="{{post.url | url}}">
                        <h2 class="post-title">
                        {{post.data.title}}
                        </h2>
                        <h5 class="post-subtitle">
                            {{post.data.description}}
                        </h5>
                        </a>
                        
                    </div>
                    <hr>
                    {% endfor %}

                    
                   </div>
                   {# <pre>{{ pagination | dump }}</pre> #}
                    <div style="text-align: center;">
                        {% if pagination.previous %}
                        <a class="btn btn-primary float-left" href="{{ pagination.previousPageLink | url }}">← Newer Posts</a>
                        {% endif %}

                        {% if pagination.next %}
                        <a class="btn btn-primary float-right" href="{{ pagination.nextPageLink | url }}">Older Posts →</a>
                        {% endif %}
                    </div>
               </section>
           
                   <section class=" sectional-content" >
                           <div class="container " id="section5">
                               <div >
                                       <h3 class="mt-4 mb-4">Get in Touch! </h3>
                                    <p class="pr-5">Want to talk about developing a new project for your business or do you have questions?
                                        <br> Regardless, feel free to contact me.</p>
                               </div>

                           <div class="row text-center mt-5 mb-5">
                               <div class="col-md-4">
                                   <i class="fas fa-map"></i> <br>
                                   <p>Uyo, Nigeria</p>
                               </div>
                               <div  class="col-md-4">
                                       <i class="fas fa-mobile-alt"></i><br>
                                   <p> + (234) 703 295 7474 </p>
                               </div>
                               <div class="col-md-4">
                                       <i class="fas fa-envelope"></i> <br>
                                   <p>
                                           aniekanedett@gmail.com 
                                   </p>
                               </div>
                           </div>
                           <div class="row mt-5 mb-5">
                              
                                <form action="https://formspree.io/f/xvovlzwb" method='POST' class='contact-form' style><input type='hidden' name='form-name' value='contact-form' />
                                    <div class="form-row" >
                                      <div class="form-group col-md-4">
                                        <label>Name</label>
                                        <input type="text" class="form-control" name="name" id="firstname" placeholder="John">
                                      </div>

                                      <div class="form-group col-md-4">
                                        <label>Email</label>
                                      <input type="email" class="form-control"  placeholder="Johndoe@example.com" id="email" name="_replyto" required="">
                                      </div>
                                    </div>

                                    <div class="form-group">
                                        <label>Mesage</label>
                                        <textarea name="message" class="form-control" rows="5" id="message"required="" data-validation-required-message="Please enter a message." placeholder="Hello, Howdy"></textarea>
                                    </div>

                                    <!-- <div class="field">
                                        <div data-netlify-recaptcha="true"></div>
                                    </div> -->

                                    <button type="submit" class="btn sendbtn">Send</button>
                                  </form>
                                 
                               </div>
                           </div>
                       </section>
    
            